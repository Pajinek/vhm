html(lang="en")
    head
      meta(http-equiv="content-type", content="text/html; charset=utf-8")
    body
        {% for it in projects %}
        h1 {{forloop.counter}}. {{ it.name }}
        h2 status: {{ it.status }}
        h3 members: {{ it.member.all|join:"," }}
        p {{ it.created }} - {{ it.deadline }}
        p {{ it.description }}
        {% endfor %}

